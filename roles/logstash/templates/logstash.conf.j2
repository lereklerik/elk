input {
    beats {
      port => 5046
      codec => json
  }
}

filter {
  json {
    source =>  "message"
  }
  date {
    match => [ "timestamp" , "dd/MMM/YYYY:HH:mm:ss Z" ]
  }
}

output {
  elasticsearch { 
    hosts => "localhost:9200"
    index => "logstash-%{+YYYY.MM.dd}"
  }
  # stdout { codec => rubydebug }
}